<%- include('partials/header', {title:"Tiger Helper"}, {data: data}) -%>

<h1> ToDo List! </h1>
<table class="table">
  <thead>
    <tr>
    <th>Categories</th>
      <% for (let day in data) { %>
    <th scope="col"> <a href="/Category/<%=day%>"><%=day%></a> </th>
    <% } %>
    </tr>
  </thead>
  <tbody>
      <% for (let categories in data["Monday"]) { %>
        <tr>
          <th>
            <%=categories%>
          </th>
          <% for (let d in data) { %>
            <td>
              <%=data[d][categories]%>
            </td>
            <%}}%>
        </tr>

    </tbody>


</table>
  <%- include('partials/footer') -%>

