<%- include('partials/header', {title:"Tiger Helper"}) -%>

<h2>High Scores</h2>
<table>
  <tr>
    <th>
      User
    </th>
    <th>
      Wins
    </th>
    <th>
      Losses
    </th>
    <th>
      Ties
    </th>
    <th>
      Win %
    </th>
  </tr>
  <%for(let i =0; i<opponents.length; i++){%>
  <th>
    <a href="/opponent/<%=opponents[i].name%>"><%=opponents[i].name%></a>
  </th>
  <th>
    <%=opponents[i].win%>
  </th>
  <th>
    <%=opponents[i].lose%>
  </th>
  <th>
    <%=opponents[i].tie%>
  </th>
  <th>
    <%=opponents[i].win_percent%>
  </th>
  </tr>
  <%}%>
</table>

  <%- include('partials/footer') -%>

